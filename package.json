{
  "name": "youtube-insight-analyzer",
  "version": "1.0.0",
  "description": "YouTube video analytics and sentiment analysis tool",
  "scripts": {
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && python app.py",
    "start:dev": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "install:all": "cd frontend && npm install && cd ../backend && pip install -r requirements.txt"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "concurrently": "^7.0.0"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.34",
    "react-remark": "^2.1.0",
    "rehype-highlight": "^7.0.2",
    "styled-components": "^6.1.15",
    "unist-util-visit": "^5.0.0"
  }
}
